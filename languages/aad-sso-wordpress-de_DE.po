msgstr "Die Azure Active Directory Single Sign-On Einstellungen wurden auf die Standardwerte zurückgesetzt."

#: SettingsPage.php:62
#@ aad-sso-wordpress
msgid "Azure Active Directory Settings"
msgstr "Azure Active Directory Einstellungen"

#: SettingsPage.php:90
#@ aad-sso-wordpress
msgid "General"
msgstr "Allgemein"

#: SettingsPage.php:97
#@ aad-sso-wordpress
msgid "Display name"
msgstr "Anzeigename"

#: SettingsPage.php:105
#@ aad-sso-wordpress
msgid "Domain hint"
msgstr "Domänen-Hinweis"

